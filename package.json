{
  "name": "node-bootstrap-template",
  "displayName": "Node Bootstrap Template",
  "version": "1.0.0",
  "description": "A starter template to bootstrap node based web services.",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cod3rguy/node-bootstrap-template.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/cod3rguy/node-bootstrap-template/issues"
  },
  "homepage": "https://github.com/cod3rguy/node-bootstrap-template#readme",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "core-js": "^3.1.4",
    "cors": "^2.7.1",
    "dotenv": "^8.0.0",
    "express": "^4.13.3",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "got": "^11.8.1",
    "http-status-codes": "^1.3.2",
    "jwks-rsa": "^1.6.0",
    "mongodb": "^3.3.0-beta2",
    "morgan": "^1.8.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.9.2"
  }
}
